{
  "alola": {
    "reading": "ア(ロー)↑ラ",
    "meaning": "こんにちは",
    "etymology": "・ポケモンSM アローラ地方のあいさつ「アローラ」より",
    "examples": [
      {
        "kapu": "alola e siali!",
        "japanese": "こんにちは、しゃりー!"
      },
      {
        "kapu": "e malil si, alola!",
        "japanese": "マリルさん、こんにちは！"
      }
    ],
    "notes": "・基本的にいつでも使えるが、昼に使うことが多い。"
  },
  "kap": {
    "reading": "(カ)↑プ",
    "meaning": "私",
    "etymology": "・ポケモン名 カプ・コケコなどから",
    "examples": [
      {
        "kapu": "kipos gu kap ga siali.",
        "japanese": "私はしゃりーです。"
      }
    ],
    "notes": "特になし"
  },
  "imalo": {
    "reading": "イ(マー)↑ロ",
    "meaning": "さようなら",
    "etymology": "・音象徴「悲しい」から",
    "examples": [
      {
        "kapu": "imalo e siali.",
        "japanese": "さようなら、しゃりー。"
      }
    ],
    "notes": "特になし"
  },
  "e": {
    "reading": "エ",
    "meaning": "[e ~で] ~へ、~に向けて",
    "etymology": "・ハワイ語より",
    "examples": [
      {
        "kapu": "neban gu kap ga nialo e siali.",
        "japanese": "私はしゃりーにおはようと言う。"
      },
      {
        "kapu": "e malil si, alola!",
        "japanese": "マリルさん、こんにちは！"
      }
    ],
    "notes": "alola e siali! などのように、eの後に名詞が続く。<br>あいさつなど、一部の状況では文頭にeを付けることができる。"
  },
  "si": {
    "reading": "(シ)↓",
    "meaning": "[~ siで] ~さん",
    "etymology": "・日本語「~氏」より<br>・韓国語「~씨」より",
    "examples": [
      {
        "kapu": "e malil si, alola!",
        "japanese": "マリルさん、こんにちは！"
      }
    ],
    "notes": "siの前には通常、人の名前が来る。"
  },
  "nialo": {
    "reading": "(ニャー)↑ロ",
    "meaning": "おはよう(起きた時)",
    "etymology": "・音象徴「猫の鳴き声」から<br>・朝、猫が飼い主を起こしにくることから",
    "examples": [
      {
        "kapu": "neban gu kap ga nialo e siali.",
        "japanese": "私はしゃりーにおはようと言う。"
      }
    ],
    "notes": "街中などで、朝に挨拶をするときはohukを使う。<br>nialoは、おやすみの朝バージョンとして考えるとよい。"
  },
  "mioat": {
    "reading": "(ミョー)↑アッ",
    "meaning": "おやすみ",
    "etymology": "・音象徴「やわらかい」から<br>・ふかふかのベッドで寝ることから",
    "examples": [
      {
        "kapu": "neban gu kiopa ga mioat.",
        "japanese": "守り神はおやすみと言う。"
      }
    ],
    "notes": "特になし"
  },
  "ohuk": {
    "reading": "(オー)↑フック",
    "meaning": "おはよう(街中などで)",
    "etymology": "よくわからない。<br>・日本語「おはよう」から？",
    "examples": [
      {
        "kapu": "kopis gu kap ga ohuk.",
        "japanese": "私はおはよう(という言葉)が好き。"
      }
    ],
    "notes": "朝目覚めたときには、nialoを使う。<br>ohukは、こんばんはの朝バージョンとして考えるとよい。"
  },
  "solk": {
    "reading": "(ソー)↑ルク",
    "meaning": "こんばんは",
    "etymology": "よくわからない。<br>・音象徴「だんだんと」から？<br>・月が昇る様子から？",
    "examples": [
      {
        "kapu": "gan gu kap zank biopet ga solk",
        "japanese": "私はこんばんはと聞くと悲しくなる。"
      }
    ],
    "notes": "特になし"
  },
  "zolia": {
    "reading": "(ゾー)↑リャ",
    "meaning": "失礼します、すみません",
    "etymology": "・なんとなく？",
    "examples": [
      {
        "kapu": "e siali si, zolia.",
        "japanese": "しゃりーさん、すみません。"
      }
    ],
    "notes": "特になし"
  },
  "malalo": {
    "reading": "マ(ラー)↑ロ",
    "meaning": "ありがとう",
    "etymology": "・ハワイ語「mahalo」より",
    "examples": [
      {
        "kapu": "malalo e kiopa.",
        "japanese": "ありがとう、守り神よ。"
      }
    ],
    "notes": "特になし"
  },
  "kalam": {
    "reading": "カーラム",
    "meaning": "ごめんなさい",
    "etymology": "・なんとなく？",
    "examples": [
      {
        "kapu": "ma, kalam.",
        "japanese": "うーん、ごめん。"
      }
    ],
    "notes": "特になし"
  },
  "siuba": {
    "reading": "(シュバー)↑",
    "meaning": "すごい",
    "etymology": "・日本語「すばらしい」から",
    "examples": [
      {
        "kapu": "mu, e leh, siuba.",
        "japanese": "いや、あなたはすごい。"
      }
    ],
    "notes": "感動詞としての使い方。<br>日本語の「すごい量」などのすごいとは違う。"
  },
  "ne": {
    "reading": "ネ",
